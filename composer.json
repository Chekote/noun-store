{
    "name": "chekote/noun-store",
    "description": "Store and work with Nouns",
    "keywords": ["noun", "testing"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phake/phake": "^4.0"
    },
    "autoload": {
        "psr-0": {
            "": [
                "src"
            ]
        }
    },
    "autoload-dev": {
        "psr-0": {
            "": [
                "test"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": false
        }
    }
}
